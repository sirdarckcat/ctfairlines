<?xml version="1.0"?>

<PropertyList>

    <path>../../../Models/FlightDeck/Overhead/anti_ice_lights.ac</path> 
    <texture-path>Aircraft/A380/Textures/Instruments</texture-path>

    <model>
        <path>Aircraft/A380/XML/FlightDeck/Overhead/guard_red.xml</path>
        <!--<name>guard_red</name>-->
        <offsets>
            <x-m>-0.071</x-m>
            <y-m>0.293</y-m>
            <z-m>0.0001s</z-m>
        </offsets>
    </model>


    <animation>
          <type>pick</type>
          <object-name>APU_MASTER</object-name>
          <action>
            <button>0</button>
            <binding>
                  <command>property-toggle</command>
                  <property>/controls/APU/run</property>
            </binding>
            <binding>
                <command>property-toggle</command>
                <property>/sim/sound/switch</property>
             </binding>
          </action>
    </animation>
    <animation>
          <type>textranslate</type>
          <object-name>APU_MASTER</object-name>
          <property>/controls/APU/run</property>
          <axis>
              <x>0</x>
              <y>0.6</y>
              <z>1</z>
          </axis>
    </animation>

    <animation>
          <type>material</type>
          <object-name>APU_MASTER</object-name>
          <emission>
                <red>0.0</red>
                <green>0.0</green>
                <blue>0.35</blue>
                <factor-prop>/controls/APU/run</factor-prop>
          </emission>
    </animation>
   
    <animation>
       <type>material</type>
       <object-name>backlight</object-name>
       <emission>
         <red>0.878</red>
         <green>0.694</green>
         <blue>0.594</blue>
         <factor-prop>/controls/lighting/panel-norm</factor-prop>
       </emission>
    </animation>

    <animation>
           <type>pick</type>
          <object-name>ENG_START</object-name>
          <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>property-adjust</command>
                <property>/controls/engines/ign-start</property>
                <step>1</step>
                <min>-1</min>
                <max>1</max>
                <wrap>false</wrap>
            </binding>
            <binding>
                  <command>property-toggle</command>
                  <property>/sim/sound/switch</property>
              </binding>
          </action>
          <action>
              <button>1</button>
              <repeatable>false</repeatable>
              <binding>
                  <command>property-adjust</command>
                  <property>/controls/engines/ign-start</property>
                  <step>-1</step>
                  <min>-1</min>
                  <max>1</max>
                  <wrap>false</wrap>
            </binding>
            <binding>
                <command>property-toggle</command>
                <property>/sim/sound/switch</property>
            </binding>
          </action>
     </animation>

     <animation>
          <type>rotate</type>
          <object-name>ENG_START</object-name>
          <property>/controls/engines/ign-start</property>
          <interpolation>
                <entry><ind>1</ind><dep>-30</dep></entry>
                <entry><ind>0</ind><dep>0</dep></entry>
                <entry><ind>-1</ind><dep>30</dep></entry>
          </interpolation>
          <center>
                <x-m>-0.11</x-m>
                <y-m>0.154</y-m>
                <z-m>0.004</z-m>
          </center>
          <axis>
                <x>0</x>
                <y>0</y>
                <z>1</z>
          </axis>
    </animation>
    
