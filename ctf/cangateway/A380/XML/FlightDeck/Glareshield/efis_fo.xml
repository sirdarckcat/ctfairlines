       </emission>
    </animation>
    
    <animation>
    <type>pick</type>
    <object-name>baro.select</object-name>
    <action>
        <button>0</button>
	<binding>
          <command>property-toggle</command>
          <property>/instrumentation/efis/baro-mode</property>
        </binding>
      </action>
  </animation>

  <animation>
  <condition>
      <equals>
        <property>/instrumentation/efis/baro-std-mode</property>
        <value>0</value>
      </equals>
    </condition>
    <type>pick</type>
    <object-name>baro.knob</object-name>
    <action>
      <button>3</button>
      <binding>
        <command>property-adjust</command>
	<property>/instrumentation/altimeter/setting-inhg</property>
	<step>0.01</step>
	<max>40.00</max>
	<min>0.00</min>
      </binding>
    </action>
    <action>
      <button>4</button>
      <binding>
        <command>property-adjust</command>
	<property>/instrumentation/altimeter/setting-inhg</property>
	<step>-0.01</step>
	<max>40.00</max>
	<min>0.00</min>
      </binding>
    </action>
  </animation>
 
   <animation>
    <type>pick</type>
